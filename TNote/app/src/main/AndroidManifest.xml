<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.superto.tnote">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_home"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity" />
        <activity android:name=".activity.BankInfo" />
        <activity android:name=".activity.BirthdayInfo" />
        <activity android:name=".activity.NoteInfo" />
        <activity android:name=".activity.AddNoteActivity" />
        <activity android:name=".activity.UpNoteActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PassActivity" />
        <activity android:name=".activity.ChanngePassActivity" />
        <activity android:name=".activity.SafetyCenterActivty" />
        <activity android:name=".activity.CryptoOperationActivity" />
        <activity android:name=".activity.AddBirthdayActivity" />
        <activity android:name=".activity.BirthdayHomeActivity"/>
        <activity android:name=".activity.UpBankActivity"/>
    </application>

</manifest>